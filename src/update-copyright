for file in *.cpp
do
  sed 's/2003-2008/2003-2009/g' $file > update-copyright.tmp
  mv update-copyright.tmp $file
done

exit

for file in *.h
do
  sed 's/59 Temple Place - Suite 330, Boston, MA 02111-1307, USA/ 51 Franklin Street, Fifth Floor, Boston, MA 02110-1301 USA/g' $file > update-copyright.tmp
  mv update-copyright.tmp $file
done
